{
  "name": "deanna-pdfs",
  "version": "1.0.0",
  "description": "Generate printable PDF catalogs from ministore image URLs",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "explore": "ts-node src/explore.ts",
    "postinstall": "npx playwright install"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
